{
  "name": "ivoglent/yii2-content-manager",
  "description": "Simplest content manager for the Yii framework",
  "keywords": [
    "ivoglent",
    "yii",
    "yii2",
    "content",
    "cms",
    "article"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/ivoglent/yii2-content-manager",
  "authors": [
    {
      "name": "Ivoglent Nguyen",
      "email": "ivoglent@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "ivoglent/yii2-media-manager": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "ivoglent\\content\\manager\\": "src"
    }
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "develop"
    }
  }
}
